{
    "name": "2amigos/yii2-leaflet-markercluster-plugin",
    "description": "Yii2 LeafLet Plugin to provide beautiful, sophisticated, high performance marker clustering solution with smooth animations and lots of great features.",
    "keywords": [
        "2amigos",
        "yii",
        "yii2",
        "yii 2",
        "extension",
        "widget",
        "maps",
        "leaflet",
        "plugin"
    ],
    "homepage": "http://yiiwheels.com/extension/leaflet-markercluster-plugin",
    "type": "yii2-extension",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "2amigOS! Consulting Group",
            "email": "hola@2amigos.us",
            "homepage": "http://2amigos.us",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/2amigos/yii2-leaflet-markercluster-plugin/issues",
        "source": "https://github.com/2amigos/yii2-leaflet-markercluster-plugin"
    },
    "require": {
        "2amigos/yii2-leaflet-extension": "~1.0",
        "bower-asset/leaflet.markercluster": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "4.*"
    },
    "autoload": {
        "psr-4": {
            "dosamigos\\leaflet\\plugins\\markercluster\\": "src"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "asset-installer-paths": {
            "bower-asset-library": "vendor/bower"
        }
    }
}
